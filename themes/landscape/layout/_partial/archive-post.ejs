<article class="archive-article archive-type-<%= post.layout %>">
  <div class="archive-article-inner">
    <header class="archive-article-header">
      <a href="<%- url_for(post.path) %>">
        <img class="archive-article-banner" src="/<%= post.slug %>/banner.png" alt="post-banner">
      </a>  
      <%- partial('post/title', {class_name: 'archive-article-title'}) %>
      <%- partial('post/archive-article-date', {class_name: 'archive-article-date', date_format: 'D MMM'}) %>
    </header>
  </div>
</article>